<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="style.css">
    <script src="/socket.io/socket.io.js"></script>
    <title>AuxBox</title>
</head>

<body>
    <div class="header"><h1>prompt</h1></div>
    <div class="container">
        <div id="playerlist">
            <ul>
                <li>players</li>
                <li>players</li>
                <li>players</li>
                <li>players</li>

            </ul>
        </div>
        <div id="searchbox">
            <input type="text" id="search" placeholder="Search for a track.." autocomplete="off">
            <div id="results"></div>
            <div id="show"></div>
        </div>
        <div id="chatbar">
            <div id="messages">messages show up here</div>
            <form id="message">
                <input name="message" autocomplete="off" type="text" required>
                <button type="submit">Send</button>
            </form>
        </div>
    </div>
    <button id="leaveroom">Leave</button>
    <template id="r">
        <div class='song' onclick="">
            <img src="" alt="image">
            <div id="title">
                <h3></h3>
            </div>
        </div>
    </template>
</body>
<script src="bundle.js"></script>

</html>